<%= form_for @address, url: student_addresses_path do |f|%>
  <p>
  <%= f.label :description %><br />
  <%= f.text_field :description %>
  </p>
  <p>
  <%= f.label :street %><br />
  <%= f.text_field :street %>
  </p>
  <p>
  <%= f.label :city %><br />
  <%= f.text_field :city %>
  </p>
  <p>
  <%= f.label :state %><br />
  <%= f.text_field :state %>
  <p>
  <%= f.label :zipcode %><br />
  <%= f.text_field :zipcode %>
  </p>
  <%= f.submit %>
<% end %>
